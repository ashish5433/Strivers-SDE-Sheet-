#include <bits/stdc++.h> 
int findMinimumCoins(int a) 
{
    // Write your code here
    int ans=0;
    ans+=a/1000;
    a%=1000;
    ans+=a/500;
    a%=500;
    ans+=a/100;
    a%=100;
    ans+=a/50;
    a%=50;
    ans+=a/20;
    a%=20;
    ans+=a/10;
    a%=10;
    ans+=a/5;
    a%=5;
    ans+=a/2;
    a%=2;
    ans+=a/1;

    return ans;

}
